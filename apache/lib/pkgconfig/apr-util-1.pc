prefix=/app/apache
exec_prefix=/app/apache
libdir=${prefix}/lib
APRUTIL_MAJOR_VERSION=1
includedir=/app/apache/include

Name: APR Utils
Description: Companion library for APR
Version: 1.4.1
# assume that apr-util requires libapr of same major version
Requires: apr-1
Libs: -L${libdir} -laprutil-${APRUTIL_MAJOR_VERSION}  /app/httpd-2.2.22/srclib/apr-util/xml/expat/libexpat.la
Cflags: -I${includedir}
